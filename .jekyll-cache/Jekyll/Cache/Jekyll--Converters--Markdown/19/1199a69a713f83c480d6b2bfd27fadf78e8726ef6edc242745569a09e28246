 (42.44, -71.15),
 (42.48, -71.23),
 (42.35, -71.05),
 (42.33, -71.11),
 (42.36, -71.25),
 (42.413, -71.143),
 (42.373917, -71.215),
 (43.37, -71.085),
 (43.39, -71.11),
 (42.29, -71.13),
 (42.31, -71.16),
 (42.313, -71.135),
 (42.297, -71.155)]
</code></pre></div></div>

<div class="language-python highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="n">radius_of_gyration</span><span class="p">(</span><span class="n">positions</span><span class="p">)</span>
</code></pre></div></div>

